{
    "name": "quansitech/qscmf-formitem-ueditor",
    "description": "qscmf ueditor form",
    "keywords": ["qscmf", "component", "form", "ueditor"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xhiny",
            "email": "axhiny@163.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "tiderjian/think-core": ">=14.0.0"
    },
    "autoload": {
        "psr-4": {
            "FormItem\\Ueditor\\": "src/"
        }
    },
    "extra": {
        "qscmf": {
            "providers": [
                "FormItem\\Ueditor\\UeditorProvider"
            ],
            "antd-admin": {
                "component": {
                    "Column.Ueditor": "antd-components/Column/Ueditor.tsx",
                    "Column.Readonly.Ueditor": "antd-components/Column/Readonly/Ueditor.tsx"
                }
            }
        }
    }
}
